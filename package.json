{
  "name": "jarsnostudios-launcher",
  "version": "1.0.0",
  "description": "A launcher for Jarsno Studios games.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "electron:build": "electron-builder"
  },
  "keywords": [],
  "author": "Jarsno Studios, techguy16",
  "license": "GPL-2.0",
  "build": {
        "appId": "com.jarsnostudios.launcher",
