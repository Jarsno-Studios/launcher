            "output": "dist"
        },
        "win": {
            "target": "zip"
        },
        "linux": {
            "category": "AudioVideo",
            "target": "AppImage"
        }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jarsno-Studios/launcher.git"
  },
  "bugs": {
